{
  "name": "femease-backend",
  "version": "1.0.0",
  "description": "Backend for FemEase women wellness application",
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "prisma generate --schema ./prisma/generated/schema.prisma",
    "dev": "npm run prisma:generate && cross-env  NODE_OPTIONS='--max-old-space-size=512' ts-node src/index.ts",
    "start": "npm run prisma:generate && cross-env  node --max-old-space-size=512 dist/index.js",
    "build": "npm run prisma:generate && npm run prisma:migrate && tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "train": "ts-node src/scripts/trainModel.ts",
    "prisma:merge": "node prisma/mergePrismaSchema.ts",
    "prisma:generate": "npm run prisma:merge && prisma generate --schema ./prisma/generated/schema.prisma",
    "prisma:migrate": "npm run prisma:merge && prisma migrate dev --schema ./prisma/generated/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@tensorflow/tfjs": "^3.21.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "format": "^0.2.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prisma": "^6.0.1",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.1",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.9",
    "@types/pg": "^8.10.9",
    "@types/sequelize": "^4.28.15",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "prisma-import": "^1.0.5",
    "prisma-merge": "^0.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "import": {
      "schemas": "./prisma/models/**/*.prisma",
      "output": "./prisma/generated/schema.prisma",
      "input": "./prisma/schema.prisma"
    }
  }
}
